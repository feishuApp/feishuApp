@font-face {
    font-family: 'iconfont';
    src: url('data:font/truetype;charset=utf-8;base64,') format('truetype');
    font-weight: normal;
    font-style: normal;
    font-display: swap;
}

.iconfont {
    font-family: "iconfont" !important;
    font-size: 22px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    color: rgb(109, 109, 109);
  }
  
