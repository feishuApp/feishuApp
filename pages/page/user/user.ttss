page{
    background-color: #e2e2e2;
}
.container{
    width: 90%;
    margin:20px auto;
}

.line{
    width: 100%;
    height: 1.5px;
    background-color: #FFA500;
    margin:15px 0;
}
.info{
    display: flex;
    flex-direction: column;
    height: 180px;
    width: 100%;
    align-items: center;
    box-shadow: 0 0 10px black;
    background-image:url('https://gimg2.baidu.com/image_search/src=http%3A%2F%2Fimg.mp.itc.cn%2Fupload%2F20170528%2F575fdbbbb8f74765851f585d90370e2b_th.jpg&refer=http%3A%2F%2Fimg.mp.itc.cn&app=2002&size=f9999,10000&q=a80&n=0&g=0n&fmt=jpeg?sec=1620393052&t=20b10f94e33f96172574fa0e56bf8b59');
    background-size: cover;
}
.avatar{
    width: 60px;
    height: 60px;
    border-radius: 50%;
    margin:30px 0 7px 0;
    box-shadow: 0 0 10px rgb(167, 166, 166);
}
.info-text{
    font-size: 20px;
    color:white;
    display: flex;
    flex-direction: column;
    align-items: center;
    font-weight: bold;
}

.small{
    font-size: 15px;
    color:rgb(204, 202, 202)
}
.bar{
    height: 30px;
    line-height: 30px;
    font-size: 17px;
    border:rgb(141, 141, 141) 1px solid;
    margin:7px;
    padding: 10px;
    background-color: #eeead9;
    border-radius: 10px;
    box-shadow: 0 0 5px rgb(161, 161, 161);
}
.icon-xiangyou{float: right;}